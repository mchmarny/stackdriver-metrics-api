#!/bin/bash

SERVICE_URL=$(gcloud beta run services describe metric-maker-service \
    --region us-central1 \
    --format="value(status.domain)")

curl -X POST \
     -H "Content-Type: application/json" \
     -d '[{
            "type": "User",
            "site_admin": false,
            "id": 1
          },{
            "type": "User",
            "site_admin": false,
            "id": 2
          },{
            "type": "User",
            "site_admin": false,
            "id": 3
          }]' \
     "${SERVICE_URL}/v1/counter/users"